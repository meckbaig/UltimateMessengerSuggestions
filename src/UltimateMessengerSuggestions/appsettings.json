{
	"Kestrel": {
		"Endpoints": {
			"Http": {
				"Url": "http://*:5101"
			}
		}
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.EntityFrameworkCore": "Information",
			"System": "Warning"
		}
	},
	"AllowedHosts": "*",
	"ConnectionStrings": {
		"Default": "Server=localhost;Port=5440;Database=ultimate_messenger;User ID=user;Password=test;"
	},
	"Application": {
		"CheckDbRetryCount": 10,
		"CheckDbRetryDelay": 60,
		"KnownProxies": [ "127.0.0.1" ]
	},
	"WebDav": {
		"Endpoint": "https://yoursite.com/remote.php/dav/files/youruser/files/yourfolder/",
		"Username": "youruser",
		"Password": "yourpassword",
		"PublicPreviewBase": "https://yoursite.com/apps/files_sharing/publicpreview/yourhash?file=/"
	},
	"Jwt": {
		"SecretKey": "YOUR_SECRET_KEY_123456",
		"Issuer": "UltimateMessengerSuggestions",
		"Audience": "UltimateMessengerSuggestionsUser"
	},
	"Seq": {
		"Enabled": true,
		"ServerUrl": "http://localhost:5341",
		"ApiKey": "",
		"MinimumLevel": "Information"
	},
	"ConsoleLog": {
		"Enabled": true,
		"MinimumLevel": "Information"
	},
	"DebugLog": {
		"Enabled": false,
		"MinimumLevel": "Debug"
	},
	"FileLog": {
		"Enabled": true,
		"MinimumLevel": "Information",
		"RetainedDaysCountLimit": 30
	}
}
